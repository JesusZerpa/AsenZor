Options +ExecCGI
AddHandler cgi-script .py

# Activar RewriteEngine
RewriteEngine on
 
# Reescribir la URL solicitada por el usuario
#   Entrada:  clientes/NOMBRE/
#   Salida: clientes.php?id=NOMBRE

Rewriterule ^(.*)/(.*)/$ controles/default/default.py?app=$1&vistas=$2
#options -Indexes

DirectoryIndex controles/default.py?app="default"&vista="index"

ErrorDocument 404 /AsenZor/controles/error404.py
#ErrorDocument 404 /AsenZor/error.html